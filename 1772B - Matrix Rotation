#include <iostream>
 
using namespace std;
 
int main()
{
    int t , a , b , c ,d;  // number of test < 1
    cin >> t ;
    while( t--)
    {
        cin >> a >> b >> c >> d ;
        if ((a<b && c <d && a<c && b<d ) || ( c<a &&d<b && c<d &&a<b)|| ( d<c && b<a &&d<b && c<a ) || ( b<d &&a<c && b<a && d<c))
        {
            cout << "YES" << endl ;
        }
 
        else
        {
            cout << "No" << endl ;
        }
                }
 
}
